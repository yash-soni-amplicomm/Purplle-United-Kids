{%- liquid
  if section.settings.header_show_currency_selector or section.settings.show_social_icons or section.settings.header_show_language_selector
    assign header_top = true 
  else 
    assign header_top = false
  endif
  assign show_theme_mode_on_header = false
  if settings.theme_mode_enabled
    assign show_theme_mode_on_header = true
    if settings.mobile_theme_mode_position == 'navigation'
      assign show_theme_mode_on_header = false
    endif
  endif
  assign search_on_header = false
  if settings.search_enabled and section.settings.search_on_header
    assign search_on_header = true
  endif
  assign areas = "heading navigation icons"
  assign areas_2 =  ". . ."
  assign class_grid = "col-span-full col-start-1 order-1 lg:mb-3"
  assign class = 'w-full justify-end'
  assign logo_above = false
  assign logo_center = false
  assign columns = 'auto 1fr auto'
  if section.settings.logo_position == 'above-center'
    assign areas =  "iconsRight heading icons" 
    assign areas_2 =  "navigation navigation navigation"
    assign class_grid = '[grid-area:iconsRight] hidden lg:block'
    if section.settings.enable_hamburger_menu
      assign class_grid = '[grid-area:iconsRight] hidden lg:block lg:mb-3'
    endif
    assign class = 'w-full'
    assign columns = '4fr 2fr 4fr'
    unless section.settings.enable_hamburger_menu
      assign logo_above = true
    endunless
  endif
  if section.settings.logo_position == 'left-center'
    assign columns = '1fr 4fr 1fr'
  endif
  if section.settings.logo_position == 'center-left'
    assign columns = '4fr 2fr 4fr'
    assign areas = ". . ."
    assign areas_2 =  "navigation heading icons"
    assign logo_center = true
  endif
  if section.settings.enable_hamburger_menu
    if header_top
      if search_on_header
        assign areas = "iconsRight iconsRight iconsRight iconsRight"
        if logo_center
          assign columns = "150px 1fr 1fr auto"
          assign areas_2 = "navigation heading search icons"
        else
          assign columns = "auto 150px 1fr auto"
          assign areas_2 = ". navigation search ."
          if section.settings.logo_position == "above-center"
            assign areas_2 = "heading navigation search icons"
          endif
        endif
      else
        assign areas = "iconsRight iconsRight iconsRight"
        if logo_center
          assign columns = "150px 1fr auto"
          assign areas_2 = "navigation heading icons"
        else
          assign columns = "auto 1fr auto"
          assign areas_2 = ". navigation ."
        endif
      endif
    else
      if search_on_header
        assign areas_2 = ". . . ."
        if logo_center
          assign columns = "150px 1fr 1fr auto"
          assign areas = "navigation heading search icons"
        else
          assign columns = "auto 150px 1fr auto"
          assign areas = ". navigation search ."
          if section.settings.logo_position == "above-center"
            assign areas = "heading navigation search icons"
          endif
        endif
      else
        assign areas_2 = ". . ."
        if logo_center
          assign columns = "150px 1fr auto"
          assign areas = "navigation heading icons"
        else
          assign columns = "auto 1fr auto"
          assign areas = ". navigation ."
        endif
      endif
    endif
  endif
  assign transparent = false
  if section.settings.enable_on_homepage and template.name == "index"
    assign transparent = true
  endif
  if section.settings.enable_on_collection_page and template.name == "collection"
    assign transparent = true
  endif
  assign class_logo = "w-[" | append: section.settings.logo_width_mobile | append: "px] md:w-[" | append: section.settings.logo_width | append: "px] h-auto"
  assign logo_name = settings.heading_base_size | times: 0.0225

  if section.settings.mobile_header_layout == 'center-left'
    assign mobile_layout = 'drawer heading icons'
  elsif section.settings.mobile_header_layout == 'center-right'
    assign mobile_layout = 'icons heading drawer'
  else
    assign mobile_layout = 'heading drawer'
  endif
-%}
{%- style -%}
  {% if section.settings.sticky_header_type == "none" or section.settings.sticky_header_type == "on-scroll-up" %}
    :root{
      --height-header: 0px;
      --announcement-height: 0px;
    }
  {% endif %}
  {%- if transparent -%}
    section:first-child {
      margin-top: 0;
    }
    #sticky-header-content {
      background: linear-gradient(to top, transparent 0%, rgba({{ section.settings.shadow_color.red }},{{ section.settings.shadow_color.green }},{{ section.settings.shadow_color.blue }}, {{ section.settings.shadow }}%) 100%);
    }
    .dark #sticky-header-content {
      {% if section.settings.shadow_color_dark.alpha != 0.0 %}
        background: linear-gradient(to top, transparent 0%, rgba({{ section.settings.shadow_color_dark.red }},{{ section.settings.shadow_color_dark.green }},{{ section.settings.shadow_color_dark.blue }}, {{ section.settings.shadow }}%) 100%);
      {% else %}
        background: transparent;
      {% endif %}
    }
    #sticky-header-content.background-header {
      background: rgb(var(--background-color-header));
    }
    @media (min-width: 768px) {
      #sticky-header-content:hover header,
      #sticky-header-content:hover {
        background: rgba(var(--background-color-header))
      }
    }
    #sticky-header-content:hover .divider {
      -colors-line-and-border: var(--background-color-header);
      --colors-line-header: var(--background-color-header);
    }
    .transparent-header .sticky-header-content:not(.sticky-header-active) .header {
      {% unless section.settings.textlink_color_light.alpha == 0.0 %}
        --colors-text-link: {{ section.settings.textlink_color_light.red }},{{ section.settings.textlink_color_light.green }},{{ section.settings.textlink_color_light.blue }};
      {% endunless %}
    }
    .dark .sticky-header-content:not(.sticky-header-active) .header {
      {%- unless section.settings.textlink_color_dark.alpha == 0.0 -%}
        --colors-text-link: {{ section.settings.textlink_color_dark.red }},{{ section.settings.textlink_color_dark.green }},{{ section.settings.textlink_color_dark.blue }};
      {% else %}  
        --colors-text-link: {{ settings.colors_dark_text_link.red }}, {{ settings.colors_dark_text_link.green }}, {{ settings.colors_dark_text_link.blue }};
      {% endunless %}
    }
  {%- endif -%}
  .logo-name {
    font-size: {{ logo_name | times: 0.5 }}rem;
  }
  .transparent-header .logo-name,
  .transparent-header header {
    {% unless section.settings.text_color_light.alpha == 0.0 %}
      color: {{ section.settings.text_color_light }};
    {% endunless %}
  }
  .dark .transparent-header .logo-name,
  .dark .transparent-header header {
    {% unless section.settings.text_color_dark.alpha == 0.0 %}
      color: {{ section.settings.text_color_dark }};
    {% else %}
      color: rgba(var(--colors-text));
    {% endunless %}
    
  }
  {% if section.settings.sticky_header_type == "reduce-logo-size" %}
    .reduce-logo-size .header-logo {
      width: 75%;
    }
  {% endif %}
  header {
    grid-template-areas: "{{ mobile_layout }}";
    {% if section.settings.mobile_header_layout == 'left-right' %}
      grid-template-columns: 3fr 3fr;
    {% endif %}
  }
  @media (min-width: 1024px) {
    header {
      grid-template-areas: "{{ areas }}" "{{ areas_2 }}";
      grid-template-columns: {{ columns }};
    } 
    .logo-name {
      font-size: {{ logo_name }}rem;
    }
    .pt-header {
      padding-top: {{ section.settings.padding_top }}px;
    }
    .pb-header {
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  .h-hamburger {
    height: var(--hamburger-menu-height);
  }
{%- endstyle -%} 
<div x-show="$store.xHeaderMenu.overlay" 
  x-transition:enter="ease-in-out duration-300"
  x-transition:enter-start="opacity-0"
  x-transition:enter-end="opacity-100"
  x-transition:leave="transition-all ease-in-out duration-300"
  x-transition:leave-end="opacity-0"
  @mouseover="$store.xHeaderMenu.hideMenu($el)"
  
  class="bg-[rgba(0,0,0,0.4)] hidden lg:flex fixed top-0 right-0 bottom-0 left-0 z-20" x-cloak>
</div>
<div
  id="x-header-container"
  class="relative text-[rgb(var(--colors-text-header))] z-50"
  x-init='$store.xHeaderMenu.removeStickyHeader();$store.xHeaderMenu.initSticky($el, "shopify-section-{{ section.id }}", "{{ section.settings.sticky_header_type }}", {{ transparent }}); $store.xHeaderMenu.mobileHeaderLayout="{{ section.settings.mobile_header_layout }}";{% if section.settings.sticky_header_type == 'on-scroll-up' %} $store.xHeaderMenu.setTopStickyHeader(){% endif %}'
  {% if section.settings.sticky_header_type != "none" %}
    x-intersect:leave="$store.xHeaderMenu.handleAlwaysSticky()"
    data-is-sticky="true"
    :class="$store.xMiniCart.open && 'z-[60]'"
  {% elsif settings.cart_type == 'drawer' %}
    :class="$store.xMiniCart.open && 'z-[60]'"
  {% endif %}
  @click.away="$store.xHeaderMenu.openHamburgerMenu = false;"
  {% if section.settings.sticky_header_type == 'on-scroll-up' or section.settings.enable_hamburger_menu %}
    @scroll.window="{% if section.settings.sticky_header_type == 'on-scroll-up' %} $store.xHeaderMenu.handelOnScrollSticky();{% endif %}
    {%- if section.settings.enable_hamburger_menu %} if($store.xHeaderMenu.scrollDir == 'down') {
      setTimeout(() => {
        requestAnimationFrame(() => {
          const submenuEls = $el.getElementsByClassName('toggle-menu');
          const visible = Array.from(submenuEls).filter(el => !el.classList.contains('toggle-menu-hidden'));
          if (visible.length == 0) {
            $store.xHeaderMenu.openHamburgerMenu = false; 
          }
        });
      }, 400);
      $store.xHeaderMenu.overlay = false;
    }{% endif %}"
  {% endif %}
>
  <div id="sticky-header" class="shopify-header text-[rgb(var(--colors-text-header))]{% if transparent %} transparent-header{% else %} background-header{% endif %}"
    {% if section.settings.sticky_header_type == 'always' or section.settings.sticky_header_type == 'reduce-logo-size' %}x-intersect="$store.xHeaderMenu.setVariableHeightHeader(true)"{% endif %}
  >
    <div
      id="sticky-header-content"
      class="sticky-header-content z-20 w-full top-0{% if transparent %} absolute disable-tranparent-collection{% endif %}
      {% if section.settings.full_width %} header-full-width{% endif %}"
      :class="{ 'background-header sticky-header-active': ($store.xHeaderMenu.isSticky || $store.xHeaderMenu.overlay{% if section.settings.enable_hamburger_menu %} || $store.xHeaderMenu.openHamburgerMenu{% endif %}), 'padding-scrollbar-header': $store.xHeaderMenu.isSticky && ($store.xPopup.open || $store.xPopup.openVideoShopping), 'static': $store.xHeaderMenu.overlay, 'relative': !$store.xHeaderMenu.overlay}"
      {% if transparent %}
        @click="$store.xHeaderMenu.clickedHeader = true; if(!window.matchMedia('(min-width: 768px)').matches) $el.classList.add('background-header');"
        @click.away="if (window.scrollY === 0){ 
          $store.xHeaderMenu.clickedHeader = false; if(!window.matchMedia('(min-width: 768px)').matches) $el.classList.remove('background-header');
        }
        "
        @mouseover="$el.classList.add('sticky-header-active'); $store.xHeaderMenu.showLogoTransparent = false;"
        @mouseleave="(!$store.xHeaderMenu.isSticky{% if section.settings.enable_hamburger_menu %} && !$store.xHeaderMenu.openHamburgerMenu{% endif %}) && !$store.xHeaderMenu.isMenuOpen && $el.classList.remove('sticky-header-active'); !$store.xHeaderMenu.isMenuOpen && $store.xHeaderMenu.showLogoTransparent = true;"
        {% if section.settings.sticky_header_type == 'none' %}
          @scroll.window =
          '
            if (window.scrollY === 0) {
              $el.classList.remove("background-header", "sticky-header-active");
              $store.xHeaderMenu.clickedHeader = false;
              $store.xHeaderMenu.showLogoTransparent = true;
            }      
          '
        {% endif %}
      {% endif %}
      x-intersect.once="$store.xHeaderMenu.setTopStickyHeader()"
    >
      <header 
        class="header lg:pb-5 pt-[{{ section.settings.padding_top_mobile }}px] pb-[{{ section.settings.padding_bottom_mobile }}px] pt-header lg:pb-0{% if section.settings.full_width %} w-full {% else %} x-pgw page-width{% endif %} grid gap-x-2{% if logo_above %} items-center{% endif %}" :class="{ '{% unless header_top %} lg:pt-5{% endunless %}': !$store.xHeaderMenu.isSticky, 'open-menu': $store.xHeaderMenu.overlay }"
        {% if section.settings.select_motion == 'hover' %}x-on:mouseleave="$store.xHeaderMenu.hideMenu($el)"{% endif %}
      >
        <div data-breakpoint="tablet" class="x-container-header-mobile-nav [grid-area:drawer] lg:hidden flex items-center w-full{% if section.settings.mobile_header_layout contains 'right' %} justify-end{% endif %}">
          {% comment %} This below is the hamburger icon for mobile screens {% endcomment %}
           <div 
            id="mobile-navigation"
            @click="setTimeout(() => { $store.xMobileNav.open() }, 0)"
            class="relative p-2 cursor-pointer"
            style="grid-area: drawer;"
          >
            <div class="{% if settings.icon_options == 'modern' %}w-5 h-5{% endif %}">
              {% render 'icon-alls', icon: 'icon-hamburger' %}
            </div>            
          </div>
          {% if section.settings.mobile_header_layout == 'left-right' %}
            <div class="flex justify-end lg:order-4 [grid-area:icons] -mr-2{% unless logo_above %} pb-header{% endunless %}">
                {% comment %} from below search icon mobile screens  {% endcomment %}
                {% comment %} <div class="flex items-center justify-center mobile-{{ section.settings.mobile_header_layout }}" 
                  @init-run.window="$store.xHeaderMenu.renderAjax($el, 'header-search-block', '#ajax-loading-header-search');"
                >
                  <a 
                    id="SearchOpen"
                    href="{{ routes.search_url }}" 
                    aria-label="search"
                    class="flex items-center cursor-pointer justify-center link focus-inset bg-none" 
                    @click.prevent="setTimeout(() => { $store.xModal.setActiveElement('SearchOpen'); }, 0)">
                    <span class="w-11 h-10 px-3 pt-2.5 pb-2">{% render 'icon-alls', icon: 'icon-search', animation_class: 'icon-animation' %}</span>
                  </a>
                </div>
                {% if settings.show_speech_search %}
                  <script src="{{ 'speech-search.js' | asset_url }}" defer></script>
                {%- endif -%} {% endcomment %}
              {%- if shop.customer_accounts_enabled -%}
                <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
                  aria-label="{{ 'customer.log_in' | t }}"
                  class="header-icon-account hidden lg:flex relative items-center justify-center">
                  <span class="w-11 h-10 pl-3 pr-2.5 pt-2.5 pb-2">
                    {% render 'icon-alls', icon: 'icon-account', animation_class: 'icon-animation' %}
                  </span>
                </a>
              {%- endif -%}
              {%- unless settings.hide_cart -%}
                <div class="header-icon-cart cursor-pointer relative flex items-center">
                  {% comment %} <div class="hidden md:block">
                    {% render 'inline-search-bar' %}
                  </div>
                   <div>
                      <a href="#swym-wishlist" class="swym-wishlist">
                        <i aria-hidden="true" focusable="false" role="presentation" class="icon icon-swym-wishlist"></i>
                        <span class="swym-wishlist-header-counter"></span>
                      </a>
                  </div> {% endcomment %}
                  <a 
                    id="cart-icon"
                    href="{{ routes.cart_url }}"
                    class="relative flex items-center"              
                    {% if settings.cart_type != 'page' %}
                      @click.prevent="setTimeout(() => { $store.xModal.setActiveElement('cart-icon'); $store.xMiniCart.openCart(); }, 0)"
                      :class="$store.xMiniCart.open && 'pointer-events-none'"
                    {% endif %}
                    aria-controls="CartDrawer"
                    aria-expanded="false"
                    aria-label="icon-cart"
                  >
                    <span class="w-11 h-10 pl-3 inline-block pr-2.5 pt-2.5 pb-2">
                      {% render 'icon-cart-all', icon: settings.cart_icon, animation_class: 'icon-animation' %}
                    </span>
                    <div id="mobile-cart-icon-bubble" class="count_item absolute -top-0.5{% if section.settings.mobile_header_layout == 'center-right' %} left-0 lg:left-auto lg:right-0 rtl:lg:right-auto rtl:lg:left-0{% else %} right-0{% endif %}" data-value="{{ cart.item_count }}">
                      <div class="bg-[rgba(var(--colors-cart))] text-[rgba(var(--colors-cart-text))] font-medium h-5 w-5 rounded-full text-[10px]{% if cart == empty %} hidden{% endif %}">
                        {%- if cart.item_count < 100 -%}
                          <span class="h-full flex items-center justify-center mt-[1px]" aria-hidden="true">{{ cart.item_count }}</span>
                        {%- else -%}
                          <span class="h-full flex items-center justify-center mt-[1px]">99+</span> 
                        {%- endif -%}
                      </div>
                    </div>
                  </a>
                </div>
              {%- endunless -%}
              {%- if settings.theme_mode_enabled -%}
                {%- if settings.theme_mode_mode == 'switch' -%}
                  <theme-mode x-data class="{% unless show_theme_mode_on_header %}hidden md:flex {% else %}flex {% endunless %}{% if settings.icon_options == 'classic' %}w-10{% else %}{% unless settings.hide_cart %}ml-1{% else %}ml-2 {% endunless %} {% unless settings.hide_cart and settings.search_enabled == false %}border-l border-solid h-fit self-center{% endunless %}{% endif %} md:w-10 items-center lg:justify-center cursor-pointer{% if section.settings.mobile_header_layout == 'center-right' %} justify-start{% else %} justify-end{% endif %}">
                    <button
                      @click.prevent="$store.xDarkMode.toggleThemeMode(); $store.xHeaderMenu.handleChangeThemeMode();"
                      class="lg:pt-2.5 lg:pb-2.5 md:pr-2.5 pl-2.5 {% if settings.icon_options == 'classic' %}button button-action pt-2.5 pb-2.5 pl-2.5 pr-2.5 rounded-full{% endif %} text-sm {% if settings.icon_options == "modern" %}{% if section.settings.mobile_header_layout == 'left-right' %}md:mr-0{% endif %}{% endif %}"                      
                      aria-label="darkmode"
                    >
                      <span class="block animate-spin {% if settings.icon_options == 'classic' %} w-4 lg:w-5{% else %} w-6{% endif %}" x-show="$store.xDOM.rePainting === $store.xDarkMode.alias" x-cloak>
                        {% render 'icon-alls', icon: 'icon-loading' %}
                      </span>

                      <span class="hidden dark:block{% if settings.icon_options == 'classic' %} w-4 lg:w-5{% else %} w-6{% endif %}" x-show="$store.xDOM.rePainting !== $store.xDarkMode.alias">
                        {% if settings.icon_options == 'classic' %}
                          <svg
                            class="svg-icon"
                            style="vertical-align: middle;fill: currentColor;overflow: hidden;"
                            viewBox="0 0 1024 1024"
                            version="1.1"
                            xmlns="http://www.w3.org/2000/svg"
                          >
                            <path d="M557.250088 959.028243c-247.490697 0-448.826192-199.306278-448.826192-446.783673 0-244.284679 198.73118-446.636317 443.002556-446.636317 0.119727 0 0.226151 0 0.346901 0 10.354835 0 19.840884 3.807722 24.477484 13.094226 4.688789 9.379625 3.593851 19.61371-2.805906 27.92398-53.604872 69.477374-81.928964 152.101163-81.928964 239.895719 0 217.308281 176.792519 393.82553 394.1008 393.82553l5.051039-0.239454c9.927093-0.134053 20.175505 5.638417 24.864294 15.043624 4.704139 9.379625 3.621481 20.603247-2.804882 28.91454C826.947554 895.269104 697.373453 959.028243 557.250088 959.028243zM497.340627 120.789017c-188.636265 29.113061-334.191338 193.506179-334.191338 389.410987 0 217.294978 176.792519 394.1008 394.1008 394.1008 104.550171 0 202.412013-40.456411 275.696086-112.540146-222.772736-26.1598-396.157645-216.07929-396.157645-445.747063C436.787506 265.754666 457.578018 188.916651 497.340627 120.789017z"  /><path d="M577.719282 865.501126c-179.75806 0-332.640006-128.359436-363.490641-305.211307-1.296529-7.441483 3.687996-14.536065 11.129478-15.833617 7.54893-1.2689 14.537088 3.687996 15.832594 11.130502 28.55229 163.705456 170.09191 282.536888 336.52857 282.536888 7.563256 0 13.682627 6.119371 13.682627 13.681604C591.40191 859.381755 585.281515 865.501126 577.719282 865.501126z"  /><path d="M222.886835 530.788925c-7.255241 0-13.307074-6.734378-13.654998-14.055111-0.306992-6.333242-0.467651-13.267165-0.467651-19.694551 0-7.562233 6.118348-13.935384 13.681604-13.935384s13.681604 5.998621 13.681604 13.560854c0 5.998621 0.160659 12.920264 0.441045 18.839067 0.361227 7.54893-5.464455 15.285125-13.013385 15.285125C223.326856 530.788925 223.112985 530.788925 222.886835 530.788925z"  /><path d="M642.680806 310.140573c4.300956 12.746302-2.566452 26.587542-15.311731 30.890544-12.747325 4.288676-26.588565-2.565429-30.891568-15.325034-4.275373-12.746302 2.565429-26.575262 15.311731-30.877241C624.56317 290.525839 638.377804 297.394271 642.680806 310.140573z"  /><path d="M801.146934 193.09788c4.275373 12.746302-2.565429 26.588565-15.337313 30.890544-12.747325 4.275373-26.561959-2.565429-30.864962-15.325034-4.300956-12.746302 2.566452-26.574239 15.311731-30.877241C783.003715 173.48417 796.844955 180.351578 801.146934 193.09788z"  /><path d="M748.131487 411.503145c4.301979 12.759605-2.565429 26.588565-15.311731 30.890544-12.746302 4.288676-26.587542-2.565429-30.891568-15.311731-4.300956-12.746302 2.566452-26.587542 15.311731-30.890544C729.987245 391.889435 743.829508 398.756843 748.131487 411.503145z"  />
                          </svg>
                        {% else %}
                          <svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewBox="0 0 24 24" fill="none">
                            <path d="M12 2C6.47715 2 2 6.47715 2 12C2 17.5229 6.47715 22 12 22C17.5229 22 22 17.5229 22 12C22 6.47715 17.5229 2 12 2ZM12 20.75C7.1675 20.75 3.25 16.8325 3.25 12C3.25 7.1675 7.1675 3.25 12 3.25C16.8325 3.25 20.75 7.1675 20.75 12C20.75 16.8325 16.8325 20.75 12 20.75Z" fill="currentColor"/>
                            <path d="M12 5.125C11.6548 5.125 11.375 5.4048 11.375 5.75V18.25C11.375 18.5952 11.6548 18.875 12 18.875C15.797 18.875 18.875 15.797 18.875 12C18.875 8.20305 15.797 5.125 12 5.125Z" fill="currentColor"/>
                          </svg>
                        {% endif %}
                      </span>
                      <span class="block dark:hidden{% if settings.icon_options == 'classic' %} w-4 lg:w-5{% else %} w-6{% endif %}" x-show="$store.xDOM.rePainting !== $store.xDarkMode.alias">
                        {% if settings.icon_options == 'classic' %}
                          <svg class="svg-icon" style="fill: currentColor;overflow: hidden;"  viewBox="0 0 1024 1024"  version="1.1" xmlns="http://www.w3.org/2000/svg">

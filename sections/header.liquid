          <div :class="$store.xHeaderMenu.openHamburgerMenu ? 'overflow-visible' : 'overflow-hidden'" class="w-full {% if section.settings.full_width %} full-width{% else %} x-pgw page-width{% endif %}">
            {%- render 'header-megamenu-block', logo_above: logo_above, logo_center: logo_center, select_motion: section.settings.select_motion, transparent: transparent, enable_hamburger: section.settings.enable_hamburger_menu -%}
          </div>
        </div>
      {% endif %}
      {%- unless template == 'cart' or settings.cart_type == 'page' -%}
        <div @init-run.window="$store.xHeaderMenu.renderAjax($el, 'cart-drawer', '#ajax-loading-cart');"></div>
      {%- endunless -%}
      
      {% render 'section-divider', classHeader: ' absolute top-full left-0 w-full' %}
    </div>
    {% unless settings.show_minicart %}
      {% render 'cart-noti', sticky_header: section.settings.sticky_header_type %}
    {% endunless %}
  </div>
</div>    
<script src="{{ 'search-prompts.js' | asset_url }}" defer></script>                   
<script type="application/ld+json">

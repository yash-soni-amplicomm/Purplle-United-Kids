                          {%- endif -%}
                        </div>
                      </div>
                      <span class="font-medium{% if section.settings.menu_font_size == 'medium' %} font-menu-level-1 ml-2 rtl:ml-0 rtl:mr-2{% else %} font-menu-level-0 ml-4 rtl:ml-0 rtl:mr-4{% endif %}">{{ 'sections.mobile-navigation.my_cart' | t }}</span>
                    </a>
                  </li>
                {%- endunless -%}
              {%- when 'account' -%}
                {%- if shop.customer_accounts_enabled -%}
                  <li class="x-block-login block-mobile-navigation block-mobile-navigation-{{ block.type }} pl-4 pr-4 block-mobile-navigation-after" {{ block.shopify_attributes }}>
                    {%- liquid
                      if customer and request.path == routes.account_url
                        assign active = true
                      endif
                      if routes.account_login_url == request.path
                        assign active = true
                      endif
                    -%}
                    <a href="{% if customer %}{{ routes.account_url }}{% else %}{{ routes.account_login_url }}{% endif %}"
                      class="{% if section.settings.menu_font_size == 'large' %}font-menu-level-0{% endif %} font-medium pt-3.5 pb-3.5 flex items-center disable-effect{% if active %} effect-inline{% endif %}"

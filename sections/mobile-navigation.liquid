{%- liquid
  assign show_custom_menu = false
  for block in section.blocks
    if block.type == 'menu' or block.type == 'accordion_menu' or block.type == 'carousel_menu'
      assign show_custom_menu = true
    endif
  endfor
  if section.settings.show_currency_selector or section.settings.show_language_selector
    assign show_language = true
  else
    assign show_language = false
  endif
  assign base_size = settings.text_base_size | times: 0.00875
  if section.settings.enable_highlight
    assign menus = section.settings.menus | newline_to_br | strip_newlines | split: '<br />'
  endif
  if settings.lang_direction contains request.locale.iso_code
    assign is_rtl = true
  else
    assign is_rtl = false
  endif
-%}
{%- style -%}
  #mobile-navigation * {
    {%- if section.settings.divider_color.alpha != 0.0 -%}
      border-color: {{ section.settings.divider_color }};
    {%- else -%}
      border-color: rgba(var(--colors-line-header));
    {%- endif -%}
  }
  .dark #mobile-navigation * {
    {%- if section.settings.divider_color_dark.alpha != 0.0 -%}
      border-color: {{ section.settings.divider_color_dark }};
    {%- else -%}
      border-color: rgba(var(--colors-line-header));
    {%- endif -%}
  }
  #mobile-navigation .text-color,
  #mobile-navigation .background-header {
    {%- if section.settings.background_color.alpha != 0.0 -%}
      background: {{ section.settings.background_color }};
    {%- else -%}
      background: rgba(var(--background-color-header));
    {%- endif -%}
    {%- if section.settings.text_color.alpha != 0.0 -%}
      --colors-heading: {{ section.settings.text_color.red }}, {{ section.settings.text_color.green }}, {{ section.settings.text_color.blue }};
      color: {{ section.settings.text_color }};
      --colors-text-header: {{ section.settings.text_color.red }}, {{ section.settings.text_color.green }}, {{ section.settings.text_color.blue }};
      {%- else -%}
      --colors-text: {{ settings.colors_text.red }}, {{ settings.colors_text.green }}, {{ settings.colors_text.blue }};
      --colors-heading: {{ settings.colors_heading.red }}, {{ settings.colors_heading.green }}, {{ settings.colors_heading.blue }};
      {%- endif -%}
      {%- if section.settings.colors_text_link.alpha != 0.0 -%}
        --colors-text-link: {{ section.settings.colors_text_link.red }}, {{ section.settings.colors_text_link.green }}, {{ section.settings.colors_text_link.blue }};
      {% else %}  
        --colors-text-link: {{ settings.colors_text_link.red }}, {{ settings.colors_text_link.green }}, {{ settings.colors_text_link.blue }};
      {% endif %}
  }
  .dark #mobile-navigation .text-color,
  .dark #mobile-navigation .background-header {
    {%- if section.settings.background_color_dark.alpha != 0.0 -%}
      background: {{ section.settings.background_color_dark }};
    {%- endif -%}
    {%- if section.settings.text_color_dark.alpha != 0.0 -%}
      --colors-heading: {{ section.settings.text_color_dark.red }}, {{ section.settings.text_color_dark.green }}, {{ section.settings.text_color_dark.blue }};
      color: {{ section.settings.text_color_dark }};
      --colors-text-header: {{ section.settings.text_color_dark.red }}, {{ section.settings.text_color_dark.green }}, {{ section.settings.text_color_dark.blue }};
      --colors-text-link: {{ section.settings.text_color_dark.red }}, {{ section.settings.text_color_dark.green }}, {{ section.settings.text_color_dark.blue }};
    {%- else -%}
      --colors-text: {{ settings.colors_dark_text.red }}, {{ settings.colors_dark_text.green }}, {{ settings.colors_dark_text.blue }};
      --colors-heading: {{ settings.colors_dark_heading.red }}, {{ settings.colors_dark_heading.green }}, {{ settings.colors_dark_heading.blue }};
    {%- endif -%}
    {%- if section.settings.colors_text_link_dark.alpha != 0.0 -%}
      --colors-text-link: {{ section.settings.colors_text_link_dark.red }}, {{ section.settings.colors_text_link_dark.green }}, {{ section.settings.colors_text_link_dark.blue }};
    {% else %}  
      --colors-text-link: {{ settings.colors_dark_text_link.red }}, {{ settings.colors_dark_text_link.green }}, {{ settings.colors_dark_text_link.blue }};
    {% endif %}
  }
  {% if section.settings.enable_highlight %}
    .svg-highlight-{{ section.id }} {
    {% if section.settings.highlight_color_light != blank and section.settings.highlight_color_light.alpha != 0.0 %}
      --color-highlight: {{ section.settings.highlight_color_light }};
    {% endif %}
    }
    .dark .svg-highlight-{{ section.id }} {
      {% if section.settings.highlight_color_dark != blank and section.settings.highlight_color_dark.alpha != 0.0 %}
        --color-highlight: {{ section.settings.highlight_color_dark }};
      {% else %}
        --color-highlight: {{ settings.colors_dark_heading_highlight }};
      {% endif %}
    }
  {% endif %}
  .font-menu-level-0 {
    font-size: {{ settings.text_base_size | times: 0.0125 }}rem;
  }
  .font-menu-level-1 {
    font-size: {{ settings.text_base_size | times: 0.01 }}rem;
  }
  .font-menu-level-2 {
    font-size: {{ settings.text_base_size | times: 0.00875 }}rem;
  }
  {% if section.settings.menu_font_size == 'medium' %}
    .block-mobile-navigation-menu .font-menu-level-0, 
    .block-mobile-navigation-menu .font-menu-level-1, 
    .block-mobile-navigation-menu .font-menu-level-2,
    .block-mobile-navigation-carousel_menu .font-menu-level-0,
    .block-mobile-navigation-carousel_menu .font-menu-level-2,
    .block-mobile-navigation-account .font-menu-level-1,
    .block-mobile-navigation-cart .font-menu-level-1
    {
      font-size: {{ settings.text_base_size | times: 0.007875 }}rem;
      font-weight: var(--font-body-weight);
    }
  {% endif %}
  .block-mobile-navigation-after::after {
    content: var(--tw-content);
    position: absolute;
    width: 100%;
    left: 0;
    border-bottom-width: 1px;
    {%- if section.settings.divider_color.alpha != 0.0 -%}
      border-color: {{ section.settings.divider_color }};
    {%- else -%}
      border-color: rgba(var(--colors-line-header));
    {%- endif -%}
  }
  .dark .block-mobile-navigation-after::after {
    {%- if section.settings.divider_color_dark.alpha != 0.0 -%}
      border-color: {{ section.settings.divider_color_dark }};
    {%- else -%}
      border-color: rgba(var(--colors-line-header));
    {%- endif -%}
  }
{%- endstyle -%}
<div
  class="z-[60] fixed top-0 h-full w-full opacity-60 bg-black"
  x-show="$store.xMobileNav.show"
  x-cloak
  @click="setTimeout(() => { $store.xMobileNav.close() }, 0)"
></div>
<template x-if="!runDispatch">
  <div
    id="mobile-navigation"
    :class="{ 'menu-opening': $store.xMobileNav.show }" 
    class="menu-drawer fixed z-[60] top-0 bottom-0 w-full max-w-xl motion-reduce overflow-x-hidden{% if is_rtl %} rtl right-0 translate-x-full{% else %} ltr left-0 -translate-x-full{% endif %}"
  >
    <div class="x-container-mobile-nav background-header gradient menu-drawer duration-300 ease-in-out overflow-hidden h-full flex flex-col">
      <div class="x-block-mobile-nav-close p-4 w-full flex justify-between items-center"
        aria-label="{{ 'accessibility.close' | t }}"
      >
        <span class="font-menu-level-2 uppercase">{{ 'header.menu' | t }}</span>
        <span
          class="cursor-pointer icon-close-container inline-block group-close-btn rounded-full absolute top-2 right-2 rtl:right-auto rtl:left-2"
          @click="setTimeout(() => { $store.xMobileNav.close() }, 0)"
        >{% render 'icon-alls', icon: 'icon-close', animation_class: 'icon-close-animation' %}</span>
      </div>
      <div
        id="menu-navigation"
        class="h-full flex-1 overflow-x-hidden scrollbar-body overflow-y-auto scrollbar-stable"
      >
        {% if settings.mobile_search_position == 'navigation' %}
          {% style %}
            .search-container-{{ section.id }} {
              width: calc(100% + 2px);
              {% if is_rtl %} 
                right: -1px;
              {% else %} 
                left: -1px;
              {% endif %}
            }
            #predictive-search-results .predictive-search-heading, #predictive-search-results .predictive-result-item {
              padding-left: 1rem;
              padding-right: 1rem;
            }

            #predictive-search-results .predictive-search-heading, .predictive-search__heading {
              font-size: {{ settings.text_base_size | times: 0.01 }}rem;    
            }

            #predictive-search-results .predictive-result-item, .predictive-search__list-item {
              font-size: {{ settings.text_base_size | times: 0.00875 }}rem;
            }
          {% endstyle %}
          {% if settings.search_enabled %}
            <div class="x-block-mobile-nav-search h-fit w-full text-left rtl:text-right">
              {% liquid
                assign search_type =  'query,' | append: settings.search_type 
                if settings.lang_direction contains request.locale.iso_code
                  assign is_rtl = true
                else
                  assign is_rtl = false
                endif
              %}
              <div 
                x-data="xModalSearch('{{ search_type }}','{{ settings.search_desktop_maximun_results }}',{{ settings.search_mobile_maximun_results }}, '{{ 'templates.search.product_type_default' | t }}')" 
                class="x-container-header-search header-search pb-4 pl-4 pr-4 border-b"
                @click.away="showSuggest = false; openResults = false;"
              >
                {%- if settings.predictive_search_enabled -%}
                  <div class="relative max-w-4xl w-full" data-loading-text="{{ 'accessibility.loading' | t }}">
                {%- endif -%}
                {% if settings.search_refined %}
                  {% style %}
                    .search-refined-item {
                      font-size: {{ settings.text_base_size | times: 0.00875 }}rem;
                    }
                  {% endstyle %}
                  <div class="relative flex items-center search-categories mb-3 max-w-[62rem] w-full border border-solid{% if settings.edges_type == 'rounded_corners' %} rounded-full{% endif %}">
                    <div 
                      class="flex shrink min-w-0 w-full pt-1 pb-1 {% if is_rtl %}pl-3 pr-5{% else %}pr-3 pl-5{% endif %}"
                      x-data="{ 
                        open: false
                      }" 
                      @keyup.escape="open = false">
                      <button 
                        @click.prevent="open = ! open;"
                        class="relative flex gap-4 items-center justify-between shrink min-w-0 w-full text-left rtl:text-right cursor-pointer hover-text-link duration-200 transition"
                        aria-expanded="false" 
                        aria-controls="CategoriesList" 
                        aria-describedby="CategoriesHeading">
                        <span class="line-clamp-1 p-break-words" x-text="productTypeSelected"></span>
                        <span class="opacity-60 none_border w-8 h-8 min-w-8 min-h-8 p-2" :class="open && ' -rotate-180'">
                          {% render 'icon-alls', icon: 'icon-caret' %}
                        </span>
                      </button>
                      <div 
                        x-show="open" 
                        @click.away="open = false" 
                        x-cloak
                      >
                        <div 
                          x-show="open" 
                          x-transition:enter="transition-all ease-in-out duration-500"
                          x-transition:enter-start="opacity-0 transform -translate-y-4"
                          x-transition:enter-end="opacity-100 transform translate-y-0"
                          x-transition:leave="transition-all ease-in-out duration-500"
                          x-transition:leave-end="opacity-0 transform -translate-y-4"
                          class="absolute z-[100] pt-6 pb-7 search-container-{{ section.id }} text-left rtl:text-right top-[calc(100%+12px)] lg:bottom-auto background-header shadow-[0_0_6px_-4px_rgba(var(--colors-text))]">
                          <ul 
                            role="list" 
                            class="w-full md:w-auto md:min-w-[15rem] pl-7 pr-7 left-0 rtl:left-auto rtl:right-0 max-h-[15rem] overflow-y-auto scrollbar-body scrollbar-visible">
                            <li 
                              tabindex="0"
                              class="search-refined-item cursor-pointer py-1 duration-200 transition text-[rgb(var(--colors-text-header))]" 
                              @click="setProductType('{{ 'templates.search.product_type_default' | t }}', 'product_type'); open = false">
                              <p
                                class="nav-link inline-block hover-text-link relative p-break-words"
                                :class="productTypeSelected == '{{ 'templates.search.product_type_default' | t }}' && 'border-b link-active'"
                                href="#"
                                data-no-instant>
                                {{ 'templates.search.product_type_default' | t }}
                              </p>
                            </li>
                            {% for product_type in shop.types %}
                              {% if product_type != blank %}
                                <li 
                                  tabindex="0"
                                  class="search-refined-item cursor-pointer py-1 duration-200 transition text-[rgb(var(--colors-text-header))]" 
                                  @keyup.enter="setProductType(`{{ product_type }}`, 'product_type');"
                                  @click="setProductType(`{{ product_type }}`, 'product_type'); open = false">
                                  <p
                                    class="nav-link inline-block hover-text-link relative p-break-words"
                                    :class="productTypeSelected == `{{ product_type }}` && 'border-b effect-inline'"
                                    href="#"
                                    data-no-instant>
                                    {{ product_type }}
                                  </p>
                                </li>
                              {% endif %}
                            {%- endfor -%}
                          </ul>
                        </div>
                      </div>
                      <input type="hidden" name="product_type" id="product_type" value="product_type:" data-disclosure-input tabindex="-1"/>
                    </div>
                  </div>
                {% endif %}
                <div class="max-w-[62rem] w-full border border-solid{% if settings.edges_type == 'rounded_corners' %} rounded-full{% endif %}">
                  <div class="relative field flex pt-1 pb-1 {% if is_rtl %}pl-3 pr-5{% else %}pr-3 pl-5{% endif %}">
                    <form id="header-form-search-{{ section.id }}" class="w-full flex gap-4" action="{{ routes.search_url }}" method="get" role="search">
                      {% style %}
                        .input-search::placeholder {
                          {% if settings.search_prompts_font == "search_prompts_font_text" %}
                            font-family: var(--font-body-family);
                          {% else %}
                            font-family: var(--font-heading-family);
                          {% endif %}
                        }
                      {% endstyle %}
                      <input 
                        x-data="xSearchPrompts($el, ['{{ settings.search_prompts_1 }}', '{{ settings.search_prompts_2 }}', '{{ settings.search_prompts_3 }}'], {% if settings.search_prompts_mobile %}true{% else %}false{% endif %})"
                        id="header-search-in-modal-mobile"
                        class="peer input-search focus-visible:outline-none w-fit grow appearance-none relative outline-none bg-transparent placeholder:text-current placeholder:opacity-60{% if settings.edges_type == 'rounded_corners' %} rounded-full{% endif %}"
                        type="search"
                        name="q"
                        value=""
                        x-on:keyup="keyUp()"
                        @keydown.tab="showSuggest = false; openResults = false;"
                        @focus="focusForm()"
                        placeholder="{{ 'general.search.search' | t }}"
                      >
                      <input type="hidden" name="options[prefix]" value="last">
                      <div>
                        {% if settings.show_speech_search %}
                          <button class="search__speech cursor-pointer w-8 h-8 min-w-8 min-h-8 p-2 opacity-60 bg-none text-current" x-data="xSpeechSearch($el)" x-init="initSpeechSearch()" x-show="show">
                            {% render 'icon-alls', icon: 'icon-voice' %}
                          </button>
                          <script src="{{ 'speech-search.js' | asset_url }}" defer></script>
                        {% endif %}
                        <button 
                          class="btn-search none_border w-8 h-8 min-w-8 min-h-8 p-2 opacity-60 bg-none text-current"
                          aria-label="{{ 'general.search.search' | t }}">
                          {%- render 'icon-alls', icon: 'icon-search', animation_class: 'icon-animation' -%}
                        </button>
                      </div>
                      {%- if settings.predictive_search_enabled -%}
                        <div 
                          :class="openResults ? 'visible' : 'invisible'"
                          id="header-search-result-mobile" class="z-[60] search-container-{{ section.id }} top-[calc(100%+12px)] w-full absolute {% if is_rtl %} rtl{% endif %}" tabindex="-1" data-predictive-search x-html="result" x-show="loading == false" 
                        >
                          <div class="predictive-search__loading-state">
                            <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66" xmlns="http://www.w3.org/2000/svg">
                              <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                            </svg>
                          </div>
                        </div>
                        {% if settings.search_recomendation != blank or settings.popular_product_search != blank %}
                          {% style %}
                            #header-suggest-search-mobile .search-title button {
                              font-size: {{ settings.text_base_size | times: 0.01 }}rem;
                            }
                          {% endstyle %}
                          <div 
                            id="header-suggest-search-mobile" class="z-[60] top-[calc(100%+12px)] w-full absolute left-0 {% if is_rtl %} rtl{% endif %}" 
                            tabindex="-1" 
                            data-predictive-search 
                            x-cloak 
                            :class="showSuggest ? 'visible' : 'invisible'"
                          >
                            {%- 
                              render 'suggest-search', 
                              search_recomendation: settings.search_recomendation, 
                              popular_product_search: settings.popular_product_search 
                            -%}
                          </div>
                        {% endif %}
                      {%- endif -%}
                    </form>
                  </div>
                </div>
                {%- if settings.predictive_search_enabled -%}
                  </div>
                {%- endif -%}
              </div>
            </div>
          {% endif %}
        {% endif %}
        <ul 
          x-data="{ openCur: 0 }"
          id="menu-level-0"
          role="list" 
          class="menu-parent relative h-full w-full" 
        >
          {% comment %}theme-check-disable LiquidTag{% endcomment %} 
          {%- unless show_custom_menu -%}
            {%- if section.settings.menu != blank -%}
              <li class="block-mobile-navigation-menu">
                <span class="flex empty:hidden border-t ml-4 mr-4"></span>
                {%- render 'mobile-main-menu',
                  links: section.settings.menu.links,
                  start_level: 0,
                  start_id: '',
                  menus: menus,
                  menu_type: 'tab'
                -%}
              </li>
            {%- endif -%}
          {%- endunless -%}
          
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when 'menu' -%}
                {%- if block.settings.heading != blank -%}
                  {%- assign index = forloop.index -%}
                  {%- liquid
                    assign link = false
                    assign is_link = false
                    if block.settings.menu_item_link
                      assign link = block.settings.menu_item_link
                    endif
                    if block.settings.menu.links != blank
                      assign is_link = true
                    endif
                  -%}
                  {% capture menu_title %}
                    {% assign title = block.settings.heading %}
                    {% if section.settings.enable_highlight %}
                      {% liquid
                        assign is_highlight = false
                        assign list_menu = menus | join: '||' | prepend: '||' | append: '||' | escape
                        assign menu_item = '||' | append: title | append: '||' | escape
                        if list_menu contains menu_item
                          assign is_highlight = true
                        endif
                      %}
                      {% if is_highlight %}
                        {% render 'menu-highlight',
                          highlight_type: section.settings.highlight_style,
                          menu_title: title,
                          section_id: section.id,
                          transparent: false
                        %}
                      {% else %}
                        <span>{{ title | escape }}</span>
                      {% endif %}
                    {% else %}
                      <span>{{ title | escape }}</span>
                    {% endif %}
                  {% endcapture %}
                  <li x-data='{ active: false }' class="x-block-menu-{{ forloop.index }} block-mobile-navigation-{{ block.type }} block-mobile-navigation block-mobile-navigation-after" {{ block.shopify_attributes }}>
                    {% capture menu_label %}
                      {%- if block.settings.label != blank -%}
                        {%- style -%}
                          .color-label-{{ block.id }} {
                            background: {{ block.settings.label_color }};
                            color: {{ block.settings.label_text_color_light }};
                            line-height: normal
                          }
                          .color-label-{{ block.id }}:before{
                            border-top-color: {{ block.settings.label_color }}; 
                          }
                          .dark .color-label-{{ block.id }} {
                            background: {{ block.settings.label_color_dark }};
                            color: {{ block.settings.label_text_color_dark }}
                          }
                          .dark .color-label-{{ block.id }}:before {
                            border-top-color: {{ block.settings.label_color_dark }};
                          }
                          .text-{{ block.id }} {
                            font-size: {{ settings.text_base_size | times: 0.0075 }}rem;
                          }
                        {%- endstyle -%}    
                        <div class="color-label-{{ block.id }} font-normal z-10 min-w-[40px] max-w-[50%] flex items-center justify-center -right-1.5 bottom-[85%] {% if settings.edges_type == "rounded_corners" %}rounded-[4px]{% endif %} pl-2 pr-2 pt-0.5 pb-0.5 text-{{ block.id }}">
                          {% if block.settings.label_icon != 'none' %}
                            {% unless block.settings.label_custom_icon == blank and block.settings.label_another_icon == blank and block.settings.label_icon == 'another_icon' %}
                              <span class="w-3.5 h-3.5 min-w-[14px] min-h-[14px] mr-1 rtl:mr-0 rtl:ml-1">
                                {% if block.settings.label_custom_icon == blank %}
                                  {% if block.settings.label_another_icon != blank and block.settings.label_icon == 'another_icon' %}
                                    {% render 'icon-new-alls', icon: block.settings.label_another_icon %}
                                  {% else %}
                                    {% render 'icon-labels-bags', icon: block.settings.label_icon %}
                                  {% endif %}
                                {% else %}
                                  {{ block.settings.label_custom_icon }}
                                {% endif %}
                              </span>
                            {% endunless %}
                          {%- endif -%}                
                          <span class="truncate">{{ block.settings.label }}</span>
                        </div>
                      {%- endif -%}
                    {% endcapture %}
                    <div {% if is_link %}@click="$store.xMobileNav.setActiveLink('{{ index }}'); $store.xMobileNav.scrollTop();"{% endif %} class="flex items-center gap-4 ml-4 mr-4{% if settings.mobile_search_position == 'header' and forloop.first and index == 1 %} border-t{% endif %}">
                      {% if is_link %}
                        <div class="flex-1 pt-3.5 pb-3.5 flex flex-wrap gap-2 items-center disable-effect{% if request.path == link %} effect-inline active{% endif %}" :class="active && 'effect-inline'">
                          <span class="p-break-words font-medium font-menu-level-0">{{ menu_title }}</span>
                          {{ menu_label }}
                        </div>
                        <span
                          class="icon-caret-mobile w-4 h-4 min-w-4 min-h-4 block -rotate-90 rtl:rotate-90{% if request.path == link %} text-[rgba(var(--colors-text-link))]{% endif %}"
                          :class="{ 'text-[rgba(var(--colors-text-link))]': active }"
                        >{% render 'icon-alls', icon: 'icon-caret' %}</span>
                      {% else %}
                        <div {% if link %}@click="window.open('{{ link }}', '{% if block.settings.open_new_window %}_blank{% else %}_self{% endif %}')"{% endif %}
                          class="flex-1 pt-3.5 pb-3.5 flex flex-wrap gap-2 items-center disable-effect{% if request.path == link %} effect-inline active{% endif %}"
                        >                        
                          <span class="p-break-words font-medium font-menu-level-0">{{ menu_title }}</span>
                          {{ menu_label }}
                        </div>
                      {% endif %}
                    </div>
                    {%- if is_link -%}
                      <div
                        class="menu-drawer fixed duration-300 ease-in-out top-0 background-header w-full h-full z-20 invisible rtl:translate-x-full -translate-x-full"
                        :class="{ 'menu-opening': $store.xMobileNav.currentMenuLinks.includes('{{ index }}') }"
                      >
                        <ul id="menu-level-1"
                          class="background-header w-full h-full overflow-auto scrollbar-body" 
                        >
                          <div class="flex sticky top-0 background-header z-30 items-center pt-4 pb-4 pl-4 pr-4 gap-4 justify-between">
                            <div class="flex flex-wrap justify-start items-center flex-1 font-medium disable-effect{% if link %}{% if link.current or link.child_active %} active{% endif %}{% endif %}">
                              <span class="w-10 h-10 min-w-[40px] min-h-10 pl-3 pr-3 pt-6 block rotate-90 rtl:-rotate-90" @click="$store.xMobileNav.removeActiveLink('{{ index }}');">
                                <span class="">{% render 'icon-alls', icon: 'icon-caret' %}</span>
                              </span>
                              <div class="flex items-center flex-1 flex-wrap"{% if link %} @click="window.open('{{ link }}', '{% if block.settings.open_new_window %}_blank{% else %}_self{% endif %}')"{% endif %}>

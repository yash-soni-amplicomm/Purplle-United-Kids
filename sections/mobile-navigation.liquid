                            {% unless block.settings.enable_overlay %}
                              {{ promotion_content }}
                            {% endunless %}
                          </div>
                        {% endif %}
                      </div>
                    {%- endif -%}
                  </li>
                {%- endif -%}
              {%- when 'cart' -%}
                {%- unless settings.hide_cart -%}

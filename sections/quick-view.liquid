{%- liquid 
  assign base_size_heading = settings.heading_base_size | times: 0.0225 
  assign font_size_heading = section.settings.title_size | times: 0.01 | times: base_size_heading
  if product == blank and request.design_mode
    assign product = section.settings.product 
  endif
  assign button_label = 'products.product.quick_view_label' | t
  if section.settings.quickview_label != blank
    assign button_label = section.settings.quickview_label | escape 
  endif
  assign product_form_id = 'product-form-' | append: section.id
-%}
{% style %}
  .button--quickview.button-solid, .button--quickview.button-disable-effect,
  .button--quickview.button-solid:before { 
    {%- unless section.settings.color_button.alpha == 0.0 -%}
      --colors-line-and-border: {{ section.settings.color_button.red }}, {{ section.settings.color_button.green }}, {{ section.settings.color_button.blue }};
      --colors-button: {{ section.settings.color_button.red }}, {{ section.settings.color_button.green }}, {{ section.settings.color_button.blue }};
    {%- else -%}
      --colors-line-and-border: var(--colors-button);
    {%- endunless -%}
    {%- unless section.settings.color_button_hover.alpha == 0.0 -%}
      --colors-button-hover: rgb({{ section.settings.color_button_hover.red }}, {{ section.settings.color_button_hover.green }}, {{ section.settings.color_button_hover.blue }});
    {%- endunless -%}
    {%- unless section.settings.color_text_button.alpha == 0.0 -%}
      --colors-button-text: {{ section.settings.color_text_button.red }}, {{ section.settings.color_text_button.green }}, {{ section.settings.color_text_button.blue }};
    {%- endunless -%}
    {%- unless section.settings.color_text_button_hover.alpha == 0.0 -%}
      --colors-button-text-hover: {{ section.settings.color_text_button_hover.red }}, {{ section.settings.color_text_button_hover.green }}, {{ section.settings.color_text_button_hover.blue }};
    {%- endunless -%}
  }
  .title-size--{{ section.id }} {
    font-size: {{ font_size_heading | times: 0.60 }}rem;
  }
  .dark .button--quickview.button-solid, .dark .button--quickview.button-disable-effect,
  .dark .button--quickview.button-solid:before { 
    {%- unless section.settings.color_button_dark.alpha == 0.0 -%}
      --colors-line-and-border: {{ section.settings.color_button_dark.red }}, {{ section.settings.color_button_dark.green }}, {{ section.settings.color_button_dark.blue }};
      --colors-button: {{ section.settings.color_button_dark.red }}, {{ section.settings.color_button_dark.green }}, {{ section.settings.color_button_dark.blue }};
    {%- else -%}
      --colors-line-and-border: var(--colors-button);
    {%- endunless -%}
    {%- unless section.settings.color_button_hover_dark.alpha == 0.0 -%}
      --colors-button-hover: rgb({{ section.settings.color_button_hover_dark.red }}, {{ section.settings.color_button_hover_dark.green }}, {{ section.settings.color_button_hover_dark.blue }});
    {%- endunless -%}
    {%- unless section.settings.color_text_button_dark.alpha == 0.0 -%}
      --colors-button-text: {{ section.settings.color_text_button_dark.red }}, {{ section.settings.color_text_button_dark.green }}, {{ section.settings.color_text_button_dark.blue }};
    {%- endunless -%}
    {%- unless section.settings.color_text_button_hover_dark.alpha == 0.0 -%}
      --colors-button-text-hover: {{ section.settings.color_text_button_hover_dark.red }}, {{ section.settings.color_text_button_hover_dark.green }}, {{ section.settings.color_text_button_hover_dark.blue }};
    {%- endunless -%}
  }
  @media (min-width: 768px) {
    .title-size--{{ section.id }} {
      font-size: {{ font_size_heading }}rem;
    }
  }
{% endstyle %}

<div 
  id="QuickView"
  x-show="$store.xQuickView.show" 
  :class="{ 'overflow-hidden padding-scrollbar': $store.xPopup.open }" 
  x-cloak 
  class="quick_view flex items-end md:items-center fixed top-0 left-0 right-0 bottom-0 z-[60]"
  @keydown.escape="$store.xQuickView.removeFocusQuickView('QuickViewButton'); $store.xQuickView.close()"
  @init-run.window="$store.xQuickView.addListener()"
  @click="$store.xQuickView.clickInsideQuickView($event)"
  x-init="if ( window.location.search.includes('_quick-view') == false ) document.querySelector('.main-container').appendChild($el.parentNode)"
>
  <div class="x-overlay absolute top-0 left-0 right-0 bottom-0 bg-[#acacac] bg-opacity-30"></div>
  <div 
    role="dialog"
    aria-label="{{ 'products.product.quick_view_label' | t }}"
    aria-modal="true"
    x-show="$store.xQuickView.show"
    class="x-quickview-modal duration-400 x-pgw page-width w-full pl-0 pr-0 md:pl-5 md:pr-5 lg:pl-16 lg:pr-16 lg:ml-auto lg:mr-auto"
    x-transition:enter="transition-all ease-in-out duration-500"
    x-transition:enter-start="opacity-0 transform translate-y-1/3"
    x-transition:enter-end="opacity-100 transform translate-y-0"
    x-transition:leave="transition-all ease-in-out duration-500"
    x-transition:leave-end="opacity-0 transform translate-y-1/3"
  >
    <div class="x-container-quickview-body pt-12 pb-5 lg:pt-8 lg:pb-8 2xl:pb-12 2xl:pt-12 relative bg-[rgba(var(--background-color),1)] md:h-auto{% if settings.edges_type == 'rounded_corners' %} rounded-t-[10px] md:rounded-[10px]{% endif %}" 
      @click.away="$store.xQuickView.close()"
      @click="$store.xQuickView.clickInsideContentQuickView($event)"
      x-intersect.full="$store.xQuickView.focusQuickView('QuickView', 'QuickViewClose')"
    >
      <button id="QuickViewClose" class="absolute group-close-btn cursor-pointer top-2 right-2 rtl:right-auto rtl:left-2 icon-close-container w-fit z-10 bg-[rgb(var(--colors-line-and-border))] rounded-full" @click.prevent="$store.xQuickView.close()">
        {% render 'icon-alls', icon: 'icon-close', animation_class: 'icon-close-animation' %}
      </button>
      <div class="x-container-quickview-content pt-2 md:pt-0 md:pl-6 md:pr-6 overflow-y-auto overflow-x-hidden scrollbar-body max-h-[calc(88vh-96px)] md:max-h-[85vh] 2xl:max-h-[77vh]">
        <div x-show="$store.xQuickView.loading" class="x-container-quickview-placeholder flex gap-x-[4%] flex-wrap animate-Xpulse{% if section.settings.desktop_layout == "right" %} flex-row-reverse{% endif %}">
          <div class="w-full md:w-[48%] bg-slate-200 mb-5 pb-[75%]"></div>
          <div class="w-full md:w-[48%]">
            {% if section.settings.show_vendor %}
              <p class="mb-2 h-4 w-10 bg-slate-200"></p>
            {% endif %}
            <p class="mb-5 h-5 w-3/4 bg-slate-200"></p>
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'price' -%}
                  <div class="h-5 bg-slate-200 w-1/3 mb-5 rounded"></div>
                {%- when 'product_sku' -%}
                  <div class="h-7 bg-slate-200 w-1/3 mb-5 rounded"></div>
                {%- when 'description' -%}
                  <div class="h-7 bg-slate-200 w-full mb-5 rounded"></div>
                {%- when 'variant_picker' -%}
                  <div class="h-7 bg-slate-200 w-2/3 mb-5 rounded"></div>
                {%- when 'buy_buttons' -%}
                  <div class="h-10 bg-slate-200 w-full mb-5 rounded"></div>
                  {%- if block.settings.show_dynamic_checkout -%}
                    <div class="h-9 bg-slate-200 w-full mb-5 rounded"></div>
                  {%- endif -%}
                {%- when 'quantity_selector' -%}
                  <div class="h-7 bg-slate-200 w-1/3 mb-5 rounded"></div>
                {%- when 'volume_pricing_table' -%}
                  <div class="h-7 bg-slate-200 w-full mb-5 rounded"></div>
                {%- when 'inventory_status' -%}
                  <div class="flex gap-2">
                    <div class="w-3 h-3 rounded-full bg-slate-200"></div>
                    <div class="w-10 h-5 rounded bg-slate-200"></div>
                  </div>
                {%- when 'collapsible_tab' -%}
                  <div class="h-7 bg-slate-200 w-full mb-5 rounded"></div>
                {%- when 'social_sharing' -%}
                  <div class="h-7 bg-slate-200 w-1/3 mb-5 rounded"></div>
                {%- when 'text' -%}
                  <div class="h-5 bg-slate-200 w-1/2 mb-5 rounded"></div>
                {%- when 'badges' -%}
                  <div class="h-5 bg-slate-200 w-20 mb-5 rounded"></div>
                {%- when 'rating' -%}
                  <div class="h-5 bg-slate-200 w-20 mb-5 rounded"></div>
                {%- when 'view_detail' -%}
                  <div class="h-5 bg-slate-200 w-20 mb-5 rounded"></div>
                {%- when 'discount_progress_bar' -%}
                  <script src="{{ 'discount-progress-bar.js' | asset_url }}" defer></script>
              {%- endcase -%}
            {%- endfor -%}
          </div>
        </div>
        <div id="quickview-product-content" class="quick-view" x-show="!$store.xQuickView.loading">
          {%- if product or request.design_mode -%}
            {%- 
              render 'product-template',
                product: product,
                product_form_id: product_form_id,
                enable_sticky_image: true,
                media_layout: 'thumbnail_slider',
                show_thumbnail_desktop: section.settings.show_thumbnails,
                transition_style: section.settings.transition_style,
                show_arrow_button: section.settings.show_arrow_button,
                show_thumbnail_mobile: false,
                quick_view: true,
                lazy_load: false,
                rounded_corner_image: section.settings.rounded_corner_image,
                show_first_image_avaiable: section.settings.show_first_image_avaiable
            -%}
          {%- endif -%}
        </div>
      </div>
    </div>
  </div>
</div>

{% if request.design_mode %}
  <div x-init="$store.xQuickView.showBtn({{ section.settings.enable_quickview }}, {{ section.settings.show_atc_button }}, '{{ button_label }}')"></div>
{% endif %}
{%- if section.settings.show_atc_button -%}
  <div class="popup-choose-options-mobile fixed z-[60] left-0 right-0 animate-goleft duration-300" x-show="$store.xQuickView.openPopupMobile" x-cloak>
    <div class="bg-[#acacac] opacity-40 block md:hidden fixed top-0 right-0 bottom-0 left-0" @click="$store.xQuickView.closePopupMobile()"></div>
    <div
      x-show="$store.xQuickView.openPopupMobile"
      class="z-30 md:hidden overflow-auto pt-2 pb-2 flex flex-col fixed bottom-0 left-0 md:bottom-auto max-h-[85vh] w-full bg-[rgba(var(--background-color),1)] text-left{% if settings.edges_type == 'rounded_corners' %} rounded-t-[10px]{% endif %}"
      x-transition:enter="transition-all ease-in-out duration-500" 
      x-transition:enter-start="opacity-0 transform translate-y-1/3" 
      x-transition:enter-end="opacity-100 transform translate-y-0" 
      x-transition:leave="transition ease-in-out duration-500" 
      x-transition:leave-end="opacity-0 transform translate-y-1/3"
    >
      <button 
        class="cursor-pointer group-close-btn bg-[rgb(var(--colors-line-and-border))] rounded-full absolute icon-close-container w-fit z-10 right-2 md:right-0 top-2 lg:end-10 rtl:left-2 rtl:md:left-0 rtl:right-auto"
        @click="$store.xQuickView.closePopupMobile()" 
      >
        {% render 'icon-alls', icon: 'icon-close', animation_class: 'icon-close-animation' %}
      </button>
      <div :class="$store.xQuickView.loadingChooseOption ? 'block md:hidden' : 'hidden md:hidden'" class="flex gap-x-[4%] flex-wrap animate-Xpulse{% if section.settings.desktop_layout == "right" %} flex-row-reverse{% endif %}">
        <div class="w-full pl-5 pr-5 pt-7 pb-5">
          <p class="mb-5 h-5 w-3/4 bg-slate-200"></p>
          <div class="h-7 bg-slate-200 w-full mb-5 rounded"></div>
          <div class="h-5 bg-slate-200 w-1/3 mb-5 rounded"></div>
          <div class="h-7 bg-slate-200 w-2/3 mb-5 rounded"></div>
          <div class="h-7 bg-slate-200 w-1/3 mb-5 rounded"></div>
          <div class="h-7 bg-slate-200 w-1/3 mb-5 rounded"></div>
          <div class="flex gap-x-5">
            <div class="h-10 bg-slate-200 flex-1 mb-5 rounded"></div>
            <div class="h-10 bg-slate-200 flex-1 mb-5 rounded"></div>
          </div>
        </div>
      </div>
      <div class="choose-options-mobile rtl:text-right" id="choose-options-mobile" x-show="!$store.xQuickView.loadingChooseOption"></div>
    </div>
  </div>
{%- endif -%}
{% liquid 
  assign atc_btn_bottom = false
  if section.settings.quickadd_mobile_position == 'bottom'
    assign atc_btn_bottom = true
  endif
  assign atc_btn_image_desktop = false
  if section.settings.quickadd_position == 'image'
    assign atc_btn_image_desktop = true
  endif
%}
<script>
window.xQuickView = {
  sectionId: '{{ section.id }}',
  enabled: {{ section.settings.enable_quickview }},
  show_atc_button: {{ section.settings.show_atc_button }},
  atc_btn_bottom: {{ atc_btn_bottom }},
  atc_btn_image_desktop: {{ atc_btn_image_desktop }},
  buttonLabel: '{{ button_label }}'
}
</script>
<script src="{{ 'quick-view.js' | asset_url }}" defer></script>
<script src="{{ 'payment-button.js' | asset_url }}" defer></script>
<script src="{{ 'product-cart.js' | asset_url }}" defer></script>
<script src="{{ 'video.js' | asset_url }}" defer></script>
<script src="{{ 'product-media.js' | asset_url }}" defer></script>
<script src="{{ 'recently-viewed.js' | asset_url }}" defer></script>
<script src="{{ 'customize-picker.js' | asset_url }}" defer></script>
<script src="{{ 'variant-select.js' | asset_url }}" defer></script>
{% schema %}

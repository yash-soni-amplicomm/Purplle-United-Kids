{%- liquid
  assign text_base_size =  settings.text_base_size | times: 0.01
-%}
{%- if section.settings.enable or request.design_mode -%}
  {%- style -%}
    {%- for block in section.blocks -%}
      {%- unless block.type == 'image-label' -%}
        .x-badge-{{ block.id }} .x-badge-content.x-badge-text{
          background-color:{{ block.settings.background_color }};
          color:{{ block.settings.text_color }};
          {% if block.settings.border_color.alpha != 0.0 %}
            border-width: 1px;
            border-style: solid;
            border-color: {{ block.settings.border_color }};
          {% endif %}
        }
        .dark .x-badge-{{ block.id }} .x-badge-content.x-badge-text{
          background-color:{{ block.settings.background_color_dark }};
          color:{{ block.settings.text_color_dark }};
          {% if block.settings.border_color_dark.alpha != 0.0 %}
            border-width: 1px;
            border-style: solid;
            border-color: {{ block.settings.border_color_dark }};
          {% else %}
            border-width: 0;
          {% endif %}
        }
      {%- endunless -%}
      .x-badge-{{ block.id }} {
        --font-size-scale: {{ block.settings.font_size_mobile | times: 0.01 | times: text_base_size }}rem;
        {% if block.settings.image != blank %}
          --width-image-label: {{ block.settings.size_mobile | times: 15 }}px;
          --height-image-label: {{ block.settings.size_mobile | times: 15 | divided_by: block.settings.image.aspect_ratio }}px;
        {% endif %}
      }
      @media (min-width: 768px) {
        .x-badge-{{ block.id }} {
          --font-size-scale: {{ block.settings.font_size | times: 0.01 | times: text_base_size }}rem;
          {% if block.settings.image != blank %}
            --width-image-label: {{ block.settings.size | times: 15 }}px;
            --height-image-label: {{ block.settings.size | times: 15 | divided_by: block.settings.image.aspect_ratio }}px;
          {% endif %}
        }
      }
    {%- endfor -%}
  {%- endstyle -%}
  {%- for block in section.blocks -%}
    {% liquid 
      assign label_position = block.settings.position | default: "custom"
      if settings.lang_direction contains request.locale.iso_code
        if label_position contains 'left'
          assign label_position = label_position | replace: 'left', 'right'
        elsif label_position contains 'right'
          assign label_position = label_position | replace: 'right', 'left'
        endif
      endif
    %}
    <div class="x-badges-block-data" x-badges-block-data='{
      "id": "{{ block.id }}",
      "enable": {{ section.settings.enable }},
      "type": "{{ block.type }}",
      "settings": {
        "show_on_product_card": {{ block.settings.show_on_product_card }},
        "show_on_product_page": {{ block.settings.show_on_product_page }},
        "applied_to": "{{ block.settings.applied_to }}",
        "applied_products": [{{ block.settings.applied_products | map: 'id' | join: ', ' }}],
        "applied_collections": [{{ block.settings.applied_collections | map: 'id' | join: ', ' }}],
        "content": "{{ block.settings.content | replace: "&", "&amp;" | replace: '"', "&#8221;" | replace: "'", "&#039;" | replace: "\", "&#8726;" }}",
        {% if block.settings.display_type != 'adapt' %}
          "type": "{{ block.settings.display_type | default: "square" }}",
        {% else %}
          {% liquid
            assign display_type = 'square'
            if settings.buttons_type == 'rounded'
              assign display_type = 'rounded-full'
            elsif settings.buttons_type == 'rounded_corners' or settings.buttons_type == 'mixed'
              assign display_type = 'round'
            endif
          %}
          "type": "{{ display_type | default: "square" }}",
        {% endif %}
        "background_color": "{{ block.settings.background_color }}",
        "text_color": "{{ block.settings.text_color }}",
        "image": {{ block.settings.image | json }},
        "image_alt": "{{ block.settings.image.alt }}",
        "image_aspect_ratio": {{ block.settings.image.aspect_ratio | default: 0 }},
        "size": {{ block.settings.size }},
        "size_mobile": {{ block.settings.size_mobile }},
        "opacity": {{ block.settings.opacity }},
        "position": "{{ label_position }}",
        "horizontal_position": {{ block.settings.horizontal_position }},
        "vertical_position": {{ block.settings.vertical_position }},
        "schedule_enabled": {{ block.settings.schedule_enabled | default: false }},
        "timezone": {{ block.settings.timezone | default: 0 }},
        "start_year": {{ block.settings.start_year | default: 0 }},
        "start_month": {{ block.settings.start_month | default: 0 }},
        "start_day": {{ block.settings.start_day | default: 0 }},
        "start_hour": {{ block.settings.start_hour | default: 0 }},
        "start_minute": {{ block.settings.start_minute | default: 0 }},
        "end_year": {{ block.settings.end_year | default: 0 }},
        "end_month": {{ block.settings.end_month | default: 0 }},
        "end_day": {{ block.settings.end_day | default: 0 }},
        "end_hour": {{ block.settings.end_hour | default: 0 }},
        "end_minute": {{ block.settings.end_minute | default: 0 }},
        "day_since": "{{ block.settings.day_start | default: 'none' }}",
        "number_show": {{ block.settings.day_end | default: 30 }}
      }
    }' x-badges-icon='{% if block.settings.icon != 'none' %}{% unless block.settings.custom_icon == blank and block.settings.another_icon == blank and block.settings.icon == 'another_icon' %}{% if block.settings.custom_icon == blank %}{% if block.settings.another_icon != blank and block.settings.icon == 'another_icon' %}{% render 'icon-new-alls', icon: block.settings.another_icon %}{% else %}{% render 'icon-labels-bags', icon: block.settings.icon | default: "none" %}{% endif %}{% else %}{{ block.settings.custom_icon }}{% endif %}{% endunless %}{% endif %}'></div>
  {%- endfor -%}
  <script src="{{ 'labels-and-badges.js' | asset_url }}" defer></script>
  {%- if request.design_mode -%}
    {%- for block in section.blocks -%}
      <div class="x-badges-block-preview" block-id="{{ block.id }}" {{ block.shopify_attributes }}></div>
    {%- endfor -%}
    <div
      class="modal fixed top-0 left-0 w-full h-full opacity-100 z-[100]"
      role="dialog"
      tabindex="-1"
      x-data="{
        isOpen: window.xBadgesPreviewOpen,
        init() {
          document.addEventListener('shopify:section:select', (event) => {
            if (event.target.id.includes('product-labels-and-badges')) {
              this.open();
            } else {
              this.close();
            }
          });

          document.addEventListener('eurus:badges:block-select', () => {
            this.open();
          });
        },
        open() {
          this.isOpen = true;
          window.xBadgesPreviewOpen = true;
        },
        close() {
          this.isOpen = false;
          window.xBadgesPreviewOpen = false;
        }
      }"
      x-show="isOpen"
      x-cloak
      x-transition
    >
      {%- liquid
        assign ratio = 'square'
        case settings.product_image_ratio
          when 'portrait'
            assign ratio = '2/3'
          when 'landscape'
            assign ratio = '4/3'
          when 'wide'
            assign ratio = '16/9'
          when '3/4'
            assign ratio = '3/4'
        endcase
      -%}
      <div class="card-product model-inner absolute !bg-[rgba(var(--background-color),1)] overflow-y-auto max-w-[90%] w-96 h-auto max-h-[90%] top-1/2 left-1/2 translate-y-[-50%] translate-x-[-50%] border">
        <div class="modal-content flex h-full relative pl-2 pr-2 pt-14 pb-6">
          <div class="close-btn icon-close-container w-fit rounded-full bg-[rgb(var(--colors-line-and-border))] group-close-btn absolute top-2 right-2 cursor-pointer" x-on:click="close">
            {% render 'icon-alls', icon: 'icon-close', animation_class: 'icon-close-animation' %}
          </div>
          {% liquid
            if settings.metafield_label != blank
              assign list_metafield_label = ''
              assign metafield_labels = settings.metafield_label | newline_to_br | strip_newlines | split: '<br />'
              for metafield_label in metafield_labels
                if forloop.last
                  assign list_metafield_label = list_metafield_label | append: '"' | append: metafield_label | append: '":"' | append: 'metafield value' | append: '"'
                else
                  assign list_metafield_label = list_metafield_label | append: '"' | append: metafield_label | append: '":"' | append: 'metafield value' | append: '",'
                endif
              endfor
            endif
          %}
          {%- capture data_for_label -%}
            {
              "title": "{{ 'product_labels_and_badges.preview_label' | t }}",
              "compare_at_price": 12000,
              "price": 8500,
              "price_with_currency": "{% if settings.currency_code_enable %}{{ 8500 | money_with_currency | escape }}{% else %}{{ 8500 | money | escape }}{% endif %}",
              "sale_amount": "{{ 3500 | money | strip_html }}",
              "inventory_management": "shopify",
              "qty": 6,
              "container": "card",
              "isXBadgesPreview": true
              {% if settings.metafield_label != blank %}
                ,"metafield_label": { 
                  {{ list_metafield_label }}
                }
              {% endif %}
            }
          {%- endcapture %}
          <div
            x-data="{ lastWindowWidth: 0 }"
            x-init="$store.xBadges.init()"
            x-intersect.once='$nextTick(() => {
              $store.xBadges.load($el);
            })'
            class="card__inner badges-preview-card w-full relative aspect-{{ ratio }}"
            x-labels-data="{{ data_for_label | escape }}"
          >
            <div class="xcard__media h-full bg-neutral-200">
              <div class="media h-full flex items-center">
                {% render 'product-placeholder' %}
              </div>
            </div>
          </div>
        </div>
        <div class="pb-8 pl-2 pr-2">
          <div class="below-image flex flex-wrap gap-1 pl-5 pr-5"></div>
          <p class="mt-2 pl-5 pr-5 full-unstyled-link link-product-variant text-[rgba(var(--colors-heading))] hover:text-[rgba(var(--colors-text-link))] cursor-pointer duration-200 p-break-words">{{ 'product_labels_and_badges.preview_label' | t }}</p>
          <div class="mt-2.5 mb-5.5 pl-5 pr-5 flex items-center flex-wrap gap-1">
            {% render 'price' %}
            <div class="leading-none next-price empty:hidden"></div>
          </div>
          <div class="bottom-card flex flex-wrap gap-1 empty:hidden pl-5 pr-5"></div>
        </div>
      </div>
    </div>
  {%- endif -%}
{%- else -%}
<script>
  document.addEventListener('alpine:init', () => {
    Alpine.store('xBadges', {
      load() {
        return;
      }
    })
  });
</script>
{%- endif -%}

                    <div id="price-sticky-{{ section.id }}" class="price-sticky-{{ section.id }} pricearea" role="status">
                      {%- render 'price',
                        product: product,
                        use_variant: true,
                        show_badges: false,
                        price_class: "pt-1"
                      -%}
                    </div>

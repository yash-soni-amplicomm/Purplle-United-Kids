                    <div id="x-estimate-delivery-container-{{ section.id }}" class="x-block-estimate-delivery w-full mb-4 md:mb-6{% if product.gift_card? or requires_shipping == false or can_show_preorder and block.settings.hide_when_pre_order %} hidden disable-estimate{% endif %}" {{ block.shopify_attributes }}>

                              name="quantity"
                              id="Quantity-sticky-{{ section.id }}"
                              min="1"
                              value="1"
                              form="{{ product_form_id_sticky }}"
                              aria-labelledby="Quantity-sticky-{{ section.id }}"
                              x-model="qty"
                              x-on:blur="invalid($el)"

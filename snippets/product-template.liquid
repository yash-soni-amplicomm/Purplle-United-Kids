                      {% endif %}
                    </div>
                  {% endif %}
                {%- when 'view_detail' -%}
                  <div class="x-block-view-detail mb-4 md:mb-5 w-full items-center flex{% if section.settings.make_content_center %} justify-center{% endif %}{% if block.settings.show_add_to_cart %} gap-3{% endif %}">
                    {% style %}

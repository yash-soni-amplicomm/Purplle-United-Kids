    background: transparent;
    border: 0;
    padding: 10px 14px;
    font-size: 14px;
    line-height: 1;
    cursor: pointer;
    color: #6d6d6d;
    font-weight: 400;
    transition: background-color 0.2s ease, color 0.2s ease;
  }

  .discount-tabs .tab-btn.active {
    background: #dedede;
    border: 1px solid #f2f2f2;
    color: #000;
    font-weight: 500;
  }

  .olr-reward-guest-login {
      margin-top: 0 !important;
  }

  #loyalty-cart-drawer .olr-c-wrap .olr-c-header h2 {
      line-height: 1.3 !important;
  }

  #loyalty-cart-drawer .olr-c-wrap .olr-reward {
      padding: 12px !important;
  }
  #loyalty-cart-drawer .olr-reward-main-total table thead th {
      padding: 8px !important;
  }
  #loyalty-cart-drawer .olr-reward-main-total table td {
  padding: 6px !important;
  }

  .olr-c-wrap .olr-reward-rede-point {
      margin-top: 0 !important;
  }
  .loyalty-cart-drawer-wrap{
    height:auto !important;
    max-height: 80% !important;
  }
  .powerd-by{
    display:none
  }
{% endstyle %}


                        <p>{{ 'sections.cart.total_savings_html' | t: total_discount: total_discount }}</p>
                        <s class="{% if is_rtl %} mr-8{% else %} ml-8{% endif %}" id="total-price">{{ total_original_price | money_with_currency }}</s>
                      </div>   

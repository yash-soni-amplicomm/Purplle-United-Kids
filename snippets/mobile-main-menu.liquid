      {% if block.settings.colors_text_link.alpha != 0.0 %}
        --colors-text-link: {{ block.settings.colors_text_link.red }}, {{ block.settings.colors_text_link.green }}, {{ block.settings.colors_text_link.blue }};
      {% else %}
        --colors-text-link: {{ settings.colors_dark_text_link.red }}, {{ settings.colors_dark_text_link.green }}, {{ settings.colors_dark_text_link.blue }};
      {% endif %}
    }
  {%- endstyle -%}
  <div class="w-full relative overflow-hidden{% if menu_type == 'accordion' %} mt-4 pb-4{% if is_rtl %} pr-3 pl-7{% else %} pl-3 pr-7{% endif %}{% else %} mt-2.5 px-4{% endif %}">
    {% capture promotion_content %}
      <div class="content-menu-level-{{ start_level | plus: 1 }} flex flex-col items-{{ block.settings.content_alignment }}{% if block.settings.enable_overlay %} absolute bottom-4 right-9 left-9{% else %} pt-4{% endif %}">
        <p class="w-max max-w-full empty:hidden nav-promotion-heading font-medium heading-{{ block.id }} text-color-{{ block.id }}">{{ block.settings.heading_promotion }}</p>

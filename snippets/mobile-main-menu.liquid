    }
    .heading-{{ block.id }} {
      font-size: {{ heading_size | times: 0.6 }}rem;
    }
    @media (min-width: 768px) {
      .heading-{{ block.id }} {
        font-size: {{ heading_size }}rem; 
      }
      .text-{{ block.id }} {
        font-size: {{ text_size }}rem;
      }
    }

      {% # theme-check-enable ImgLazyLoading %}
      {% if block.settings.enable_overlay %}
        {{ promotion_content }}
      {% endif %}
    </div>
    {% unless block.settings.enable_overlay %}
      {{ promotion_content }}
    {% endunless %}
  </div>
